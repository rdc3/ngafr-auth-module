{
  "name": "ngafr-modules",
  "version": "1.0.0",
  "description": "This library is used to add the firebase authentication (Email / Password or Social login) using the directive *ngafr-auth in the html template of Angular project.",
  "author": "Rohan Dcunha",
  "repository": {
    "type": "git",
    "url": "https://github.com/rdc3/ngafr"
  },
  "bugs": {
    "url": "https://github.com/rdc3/ngafr/issues"
  },
  "homepage": "https://rdc3.githut.io/ngafr-authentication",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngafr-testapp",
    "build": "npm run build:auth && npm run build:app",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:app1": "npm run build:auth && ng build ngafr-testapp --prod --delete-output-path false --outputPath docs --baseHref /ngafr-authentication/ && npm run create404page",
    "build:app": "ng build ngafr-testapp --outputPath docs --baseHref /ngafr-authentication/ --delete-output-path false",
    "create404page": "ncp docs/index.html docs/404.html",
    "build:auth": "ng build ngafr-authentication",
    "build:auth-watch": "ng build ngafr-authentication --watch",
    "prebuild": "npm run build:auth",
    "postbuild": "npm run create404page"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.13",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~9.1.13",
    "@angular/compiler": "~9.1.13",
    "@angular/core": "~9.1.13",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~9.1.13",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.1.13",
    "@angular/platform-browser-dynamic": "~9.1.13",
    "@angular/router": "~9.1.13",
    "firebase": "^7.24.0",
    "firebaseui": "^4.7.1",
    "firebaseui-angular": "^5.1.1",
    "hammerjs": "^2.0.8",
    "hark": "^1.2.3",
    "rxjs": "~6.6.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.13",
    "@angular-devkit/build-ng-packagr": "~0.901.13",
    "@angular/cli": "^9.1.13",
    "@angular/compiler-cli": "~9.1.13",
    "@angular/language-service": "~9.1.13",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ncp": "^2.0.0",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3"
  }
}
