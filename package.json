{
  "name": "ngafr-modules",
  "version": "1.0.0",
  "description": "This library is used to add the firebase authentication (Email / Password or Social login) using the directive *ngafr-auth in the html template of Angular project.",
  "author": "Rohan Dcunha",
  "repository": {
    "type": "git",
    "url": "https://github.com/rdc3/ngafr"
  },
  "bugs": {
    "url": "https://github.com/rdc3/ngafr/issues"
  },
  "homepage": "https://rdc3.githut.io/ngafr-authentication",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngafr-testapp",
    "build": "npm run build:auth && npm run build:app",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:app1": "npm run build:auth && ng build ngafr-testapp --prod --delete-output-path false --outputPath docs --baseHref /ngafr-authentication/ && npm run create404page",
    "build:app": "ng build ngafr-testapp --outputPath docs --baseHref /ngafr-authentication/ --delete-output-path false",
    "create404page": "ncp docs/index.html docs/404.html",
    "build:auth": "ng build ngafr-authentication",
    "build:auth-watch": "ng build ngafr-authentication --watch",
    "prebuild": "npm run build:auth",
    "postbuild": "npm run create404page",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.1",
    "@angular/cdk": "~11.1.1",
    "@angular/common": "~11.1.1",
    "@angular/compiler": "~11.1.1",
    "@angular/core": "~11.1.1",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~11.1.1",
    "@angular/material": "^11.1.1",
    "@angular/platform-browser": "~11.1.1",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/router": "~11.1.1",
    "firebase": "^8.2.4",
    "firebaseui": "^4.7.1",
    "firebaseui-angular": "^5.1.1",
    "hammerjs": "^2.0.8",
    "hark": "^1.2.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/cli": "^11.1.1",
    "@angular/compiler-cli": "~11.1.1",
    "@angular/language-service": "~11.1.1",
    "@types/jasmine": "^3.6.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.22",
    "codelyzer": "^6.0.1",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.0.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ncp": "^2.0.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.1.3"
  }
}
